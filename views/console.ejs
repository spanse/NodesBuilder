        ////Register User////
        function registerUser() {
            var data = {};
            data.username = "test";
            data.email = "this@email.ca";
            data.password = "krabbypatty";

            $.ajax({
                type: 'POST',
                data: JSON.stringify(data),
                contentType: 'application/json',
                url: '/registerUser',
                success: function(data) {
                    console.log(data);
                    console.log("Success");

                }

            });
        }

        ////Logout User////
        function logoutUser() {

            var username = $('#username').val();
            var password = $('#password').val();
            var email = $('#email').val();
            var data = {};
            data.username = username;
            data.email = email;
            data.password = password;

            $.ajax({
                type: 'GET',
                data: JSON.stringify(data),
                contentType: 'application/json', //THIS IS NOT OPTIONAL
                url: '/logout',
                success: function(data) {
                    window.location.href = '/login';
                },
                error: function(data) {
                    console.log("Logout failed");
                    console.log(data);

                }
            });
        } //createUser
    </script>
